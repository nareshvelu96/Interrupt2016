<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:fbutton="http://schemas.android.com/apk/res-auto">
    <android.support.v7.widget.CardView
        android:id="@+id/card_view"
        android:layout_gravity="center"
        android:layout_width="fill_parent"
        android:layout_height="85dp"
        android:layout_margin="5dp">
        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

            <TextView
                android:id="@+id/textVie"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="SRINIVASAN"
                android:textSize="35dp"
                android:textStyle="bold"
                android:layout_alignParentTop="true"/>
            <View
                android:layout_width="300dp"
                android:layout_marginTop="40dp"
                android:layout_height="1dp"
                android:background="@color/colorPrimaryDark" />


            <TextView
                android:id="@+id/textVie1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="FACULTY INCHARGE"
                android:layout_marginTop="10dp"
                android:layout_below="@+id/textVie"
                android:textSize="20dp" />
            <ImageButton
                android:layout_marginLeft="300dp"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:src="@drawable/adithyacall"

                />
        </RelativeLayout>
    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:id="@+id/card_view2"
        android:layout_gravity="center"
        android:layout_width="fill_parent"
        android:layout_below="@+id/card_view1"
        android:layout_height="80dp"
        android:layout_margin="5dp">

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

            <TextView
                android:id="@+id/textViewer"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="HARI SIDDHARTH"
                android:textSize="30dp"
                android:textStyle="bold"
                android:layout_alignParentTop="true"/>
            <View
                android:layout_width="300dp"
                android:layout_marginTop="40dp"
                android:layout_height="1dp"
                android:background="@color/colorPrimaryDark" />


            <TextView
                android:id="@+id/textViewr1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="VICE PRESIDENT"
                android:layout_marginTop="10dp"
                android:layout_below="@+id/textViewer"
                android:textSize="20dp" />
            <ImageButton
                android:layout_marginLeft="300dp"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:src="@drawable/adithyacall"
                />
        </RelativeLayout>
    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:id="@+id/card_view3"
        android:layout_gravity="center"
        android:layout_width="fill_parent"
        android:layout_below="@+id/card_view2"
        android:layout_height="80dp"
        android:layout_margin="5dp">

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

            <TextView
                android:id="@+id/textViewing1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="THIRUVENKATAM"
                android:textSize="30dp"
                android:textStyle="bold"
                android:layout_alignParentTop="true"/>
            <View
                android:layout_width="300dp"
                android:layout_marginTop="40dp"
                android:layout_height="1dp"
                android:background="@color/colorPrimaryDark" />


            <TextView
                android:id="@+id/textViewing2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="SECRETARY"
                android:layout_marginTop="10dp"
                android:layout_below="@+id/textViewing1"
                android:textSize="20dp" />
            <ImageButton
                android:layout_marginLeft="300dp"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:src="@drawable/adithyacall"
                />
        </RelativeLayout>
    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:id="@+id/card_view1"
        android:layout_gravity="center"
        android:layout_width="fill_parent"
        android:layout_below="@+id/card_view"
        android:layout_height="80dp"
        android:layout_margin="5dp">

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ADITHYA M"
                android:textSize="30dp"
                android:textStyle="bold"
                android:layout_alignParentTop="true"/>
            <View
                android:layout_width="300dp"
                android:layout_marginTop="40dp"
                android:layout_height="1dp"
                android:background="@color/colorPrimaryDark" />


            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="PRESIDENT"
                android:layout_marginTop="10dp"
                android:layout_below="@+id/textView"
                android:textSize="20dp" />
            <ImageButton
                android:layout_marginLeft="300dp"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:src="@drawable/adithyacall"
                />
        </RelativeLayout>
    </android.support.v7.widget.CardView>
    <android.support.v7.widget.CardView
        android:id="@+id/card_viewer"
        android:layout_gravity="center"
        android:layout_width="fill_parent"
        android:layout_below="@id/card_view3"
        android:layout_height="160dp"
        android:layout_margin="5dp">
        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">
            <info.hoang8f.widget.FButton
                android:id="@+id/feedback"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:layout_gravity="left|top"
                android:text="FEEDBACK"
                android:textColor="#FFFFFF"
                fbutton:buttonColor="#e75c5c"
                fbutton:shadowEnabled="true"
                android:textStyle="bold|italic"
                android:textSize="25dp"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"
                android:layout_alignParentStart="true" />
            <info.hoang8f.widget.FButton
                android:id="@+id/back"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:layout_gravity="left|top"
                android:text="FB"

                android:layout_below="@id/feedback"
                android:textColor="#FFFFFF"
                fbutton:buttonColor="@color/colorPrimary"
                fbutton:shadowEnabled="true"
                android:textStyle="bold|italic"
                android:textSize="25dp"
                android:onClick="fb"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"
                android:layout_alignParentStart="true" />


        </RelativeLayout>
    </android.support.v7.widget.CardView>




</RelativeLayout>